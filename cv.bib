 bdsk-url-2 = {http://esoads.eso.org/cgi-bin/bib_query?arXiv:1607.03535},
 date-added = {2016-07-14 05:37:48 +0000},
 date-modified = {2016-07-14 05:37:48 +0000},
 doi = {10.1051/0004-6361/201628777},
