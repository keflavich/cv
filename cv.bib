 author = {{McLeod}, A.~F. and {Gritschneder}, M. and {Dale}, J.~E. and \myname{Ginsburg}, A. and {Klaassen}, P.~D. and {Mottram}, J.~C. and {Preibisch}, T. and {Ramsay}, S. and {Reiter}, M. and {Testi}, L.},
 bdsk-file-1 = {},
 bdsk-url-1 = {http://arxiv.org/abs/1608.00005v1},
 bdsk-url-2 = {http://esoads.eso.org/cgi-bin/bib_query?arXiv:1608.00005},
