 author = {{Ellsworth-Bowers}, T.~P. and {Glenn}, J. and {Rosolowsky}, E. and {Mairs}, S. and {Evans}, II, N.~J. and {Battersby}, C. and \myname{Ginsburg}, A. and {Shirley}, Y.~L. and {Bally}, J.},
